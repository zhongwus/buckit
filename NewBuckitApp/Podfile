  pod 'SwiftyJSON'
  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
  
  pod 'MaterialComponents'
  pod 'Koloda', '~> 4.3.1'
  pod 'SideMenu'
  pod 'Cloudinary', '~> 2.0'
  pod 'RandomKit', '~> 5.2.3'


  # Pods for BuckitTestingWithArchitecture

  target 'NewBuckitAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'NewBuckitAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
